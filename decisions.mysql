--
-- Table structure for table `decisions`
--

CREATE TABLE `decisions` (
  `nid` int(10) NOT NULL,
  `quorum` int(10) NOT NULL,
  `mode` varchar(32) NOT NULL,
  PRIMARY KEY  (`nid`)
) ENGINE=MyISAM /*!40100 DEFAULT CHARACTER SET utf8 */;

-- --------------------------------------------------------

--
-- Table structure for table `decisions_electoral_list`
--

CREATE TABLE `decisions_electoral_list` (
  `nid` int(10) NOT NULL,
  `uid` int(10) NOT NULL,
  PRIMARY KEY  (`nid`,`uid`)
) ENGINE=MyISAM /*!40100 DEFAULT CHARACTER SET utf8 */;

-- --------------------------------------------------------

--
-- Table structure for table `decisions_options`
--

CREATE TABLE `decisions_options` (
  `nid` int(10) NOT NULL,
  `opttext` text NOT NULL,
  `optorder` int(2) default NULL,
  PRIMARY KEY  (`nid`, `optorder`),
  KEY `optorder` (`optorder`)
) ENGINE=MyISAM /*!40100 DEFAULT CHARACTER SET utf8 */;
